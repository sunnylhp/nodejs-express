<div class="container">
    <table>
        <thead>
        <tr>
            <th>标题</th>
            <th>类别</th>
            <th>评论</th>
            <th>年龄</th>
            <th>是否复制</th>
        </tr>
        </thead>
        <tbody>
        {{#each items}}
            <tr>
                <td>{{title}}</td>
                <td>{{category}}</td>
                <td>{{comment}}</td>
                <td>{{private}}</td>
                <td>{{reprint}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>